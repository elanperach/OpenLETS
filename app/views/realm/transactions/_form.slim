= simple_form_for([:admin, resource], wrapper: :foundation_inline) do |f|
  = f.error_notification
  - f.object.errors.full_messages.each do |message|
    p= message  
  - resource.admin_form_attribute_names.each do |attr|
    = form_input(f, attr)
  .row
    .large-3.columns
    .large-9.columns
      = f.button :submit, t('forms.update')